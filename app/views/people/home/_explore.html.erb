<div class="px-4" style="">
  <div class="md:mb-4">
    <div class="flex flex-col gap-2 text-stone-500">
      <% User.limit(20).find_each do |user| %>
        <div class=" text-sm flex items-center">
          <%= rand_avatar %>
          <div class="ml-[10px]"><a href="#" class="hover:text-orange-700 dark:text-zinc-400 dark:hover:text-orange-500"><%= user.nickname %></a></div>
        </div>
      <% end %>
    </div>
  </div>
</div>
